{
  "name": "react-blog",
  "version": "1.0.0",
  "description": "React,Nodejs,MongoDB blog project",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --timeout 2000 --require should --reporter spec server/test/main.js",
    "clean": "rimraf public/assets",
    "start": "NODE_ENV=dev node server/index.js",
    "pro": "NODE_ENV=production node server/index.js",
    "build:webpack:pro": "NODE_ENV=production webpack --config ./webpack/webpack.config.prod.js --progress --colors -p",
    "build:webpack": "NODE_ENV=development webpack --config ./webpack/webpack.config.dev.js --progress --colors -p",
    "buildpro": "npm run clean && npm run build:webpack:pro",
    "build": "npm run clean && npm run build:webpack",
    "postinstall": "npm run build"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "author": "youchao",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.5.2",
