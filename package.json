    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.5.2",
    "backdraft-js": "0.0.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.0",
    "bootstrap-wysiwyg": "^1.0.4",
    "config": "^1.19.0",
    "cookie-parser": "^1.4.1",
    "css-loader": "^0.23.1",
    "draft-js": "^0.1.0",
    "es6-promise": "^3.1.2",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "graceful-logger": "^0.4.3",
    "isomorphic-fetch": "^2.2.1",
    "jade": "^1.11.0",
    "jquery": "^2.2.0",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "lodash": "^4.5.1",
    "mocha": "^2.4.5",
